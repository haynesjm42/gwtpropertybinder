<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="propertybindersample">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.haynesjm42.propertybinder.PropertyBinder"/>
    
    <entry-point class="com.haynesjm42.propertybindersample.client.SampleEntryPoint"/>

    <!-- a simple string property -->
    <define-property name="testProp" values="val1,val2"/>
    <set-property name="testProp" value="val1"/>

    <!-- these values will map to an enum -->
    <define-property name="enumeratedValues" values="Value1,Value2,Value3"/>
    <set-property name="enumeratedValues" value="Value2"/>

    <set-property name="user.agent" value="safari"/>
</module>